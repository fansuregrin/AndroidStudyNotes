<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.UILayoutTest"
        tools:targetApi="31">
        <activity
            android:name=".ListViewActivity"
            android:exported="false"
            android:label="@string/list_view" />
        <activity
            android:name=".CustomLayoutActivity"
            android:exported="false"
            android:label="@string/custom_layout" />
        <activity
            android:name=".IncludeLayoutActivity"
            android:exported="false"
            android:label="@string/include_layout" />
        <activity
            android:name=".ConstraintLayoutActivity"
            android:exported="false"
            android:label="@string/constraint_layout" />
        <activity
            android:name=".FrameLayoutActivity"
            android:exported="false"
            android:label="@string/frame_layout" />
        <activity
            android:name=".RelativeLayoutActivity2"
            android:exported="false"
            android:label="@string/relative_layout_2" />
        <activity
            android:name=".RelativeLayoutActivity"
            android:exported="false"
            android:label="@string/relative_layout" />
        <activity
            android:name=".LayoutWeightActivity"
            android:exported="false"
            android:label="@string/layout_weight" />
        <activity
            android:name=".LinearLayoutActivity"
            android:exported="false"
            android:label="@string/linear_layout" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>